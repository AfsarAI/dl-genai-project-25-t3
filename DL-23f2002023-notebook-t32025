has provided current_notebook_name which is assumed to be the filename of this notebook.\n","notebook_src = current_notebook_name # e.g., 'DL-23f2002023-notebook-t32025'\n","\n","# Mount Google Drive\n","print(\"[INFO] Mounting Google Drive...\")\n","drive.mount('/content/gdrive')\n","\n","# Define the expected path in Google Drive\n","# Assuming the notebook is in the root of 'My Drive'\n","drive_notebook_path = os.path.join('/content/gdrive/MyDrive/', notebook_src)\n","\n","# Check if the notebook exists in Google Drive\n","if not os.path.exists(drive_notebook_path):\n","    print(f\"[ERROR] The notebook file '{notebook_src}' was not found in your Google Drive at '{drive_notebook_path}'.\")\n","    print(\"Please ensure the notebook is saved with the exact filename in your Google Drive, or adjust the 'drive_notebook_path' if it's in a subfolder.\")\n","    raise FileNotFoundError(f\"Notebook '{notebook_src}' not found in Google Drive.\")\n","\n","# Copy the notebook from Google Drive to the current working directory /content/\n","print(f\"[INFO] Copying '{notebook_src}' from Google Drive to current directory (/content/).\")\n","shutil.copy(drive_notebook_path, './')\n","\n","# Now, the notebook should be in the current directory, proceed with original copy logic\n","notebook_dest = os.path.join(repo_dir, notebook_src)\n","\n","print(f\"[INFO] Copying {notebook_src} to {notebook_dest}\")\n","shutil.copy(notebook_src, notebook_dest)\n","print(f\"[INFO] {notebook_src} copied to {repo_dir}/\")"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["[INFO] Mounting Google Drive...\n"]}]}]}